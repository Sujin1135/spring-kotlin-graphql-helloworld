type Query {
    students: [Student]
    startupLogs: [StartupLog]
}

type Student {
    id: String,
    city: String,
    fname: String,
}

type StartupLog {
    id: String,
    hostname: String,
    pid: String,
    startTime: String,
    buildInfo: BuildInfo,
}

type BuildInfo {
    debug: Boolean,
    gitVersion: String,
    javascriptEngine: String,
    maxBsonObjectSize: Int,
    sysInfo: String,
    version: String,
    versionArray: [String],
}
